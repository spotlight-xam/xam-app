{
  "cli": {
    "version": ">= 3.7.1"
  },
  "build": {
    "local": {
      "developmentClient": true,
      "distribution": "internal",
      "env": { "APP_VARIANT": "local" },
      "channel": "local",
      "ios": { "resourceClass": "m-medium", "simulator": true }
    },
    "dev": {
      "distribution": "internal",
      "env": { "APP_VARIANT": "dev" },
      "channel": "dev",
      "ios": { "resourceClass": "m-medium" }
    },
    "production": {
      "env": { "APP_VARIANT": "production" },
      "ios": { "resourceClass": "m-medium" },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
